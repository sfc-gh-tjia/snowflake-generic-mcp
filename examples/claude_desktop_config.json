{
  "mcpServers": {
    "snowflake-uv": {
      "command": "/opt/homebrew/bin/uv",
      "args": [
        "--directory",
        "/path/to/your/snowflake-mcp-server",
        "run",
        "snowflake_mcp_server.py"
      ]
    },
    "snowflake-python-venv": {
      "command": "/path/to/your/snowflake-mcp-server/venv/bin/python",
      "args": ["/path/to/your/snowflake-mcp-server/snowflake_mcp_server.py"]
    }
  }
} 